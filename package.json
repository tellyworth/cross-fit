{
  "name": "cross-fit",
  "version": "0.1.0",
  "description": "End-to-end testing tool for WordPress using WordPress Playground",
  "type": "module",
  "scripts": {
    "test": "node scripts/run-tests.mjs",
    "test:ui": "node scripts/run-tests.mjs --ui",
    "test:headed": "HEADLESS=false node scripts/run-tests.mjs",
    "test:internal": "INCLUDE_INTERNAL=1 node scripts/run-tests.mjs --grep @internal"
  },
  "keywords": [
    "wordpress",
    "e2e",
    "testing",
    "playground"
  ],
  "author": "",
  "license": "GPL-3.0",
  "dependencies": {
    "@wp-playground/cli": "^3.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0"
  }
}

